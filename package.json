{
    "name": "fleetiq-pro",
    "version": "1.0.0",
    "description": "AI-Powered Fleet Management System",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "build": "npm run build:frontend",
      "build:frontend": "cd frontend && npm run build",
      "install:all": "npm install && cd frontend && npm install",
      "dev:full": "concurrently \"npm run dev\" \"cd frontend && npm start\"",
      "test": "jest",
      "lint": "eslint .",
      "format": "prettier --write .",
      "render:build": "./build.sh",
      "render:start": "npm start",
      "postinstall": "cd frontend && npm install",
      "heroku-postbuild": "cd frontend && npm install && npm run build"
    },
    "keywords": [
      "fleet-management",
      "ai",
      "tensorflow",
      "openai",
      "autonomous-vehicles",
      "hackathon"
    ],
    "author": "FleetIQ Team",
    "license": "MIT",
    "dependencies": {
      "express": "^4.18.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "openai": "^4.20.1",
      "@tensorflow/tfjs-node": "^4.13.0",
      "helmet": "^7.1.0",
      "morgan": "^1.10.0",
      "express-rate-limit": "^7.1.5",
      "path": "^0.12.7",
      "compression": "^1.7.4"
    },
    "devDependencies": {
      "nodemon": "^3.0.1",
      "concurrently": "^8.2.2",
      "jest": "^29.7.0",
      "eslint": "^8.52.0",
      "prettier": "^3.0.3"
    },
    "engines": {
      "node": ">=16.0.0",
      "npm": ">=8.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/fleetiq-pro.git"
    },
    "bugs": {
      "url": "https://github.com/your-username/fleetiq-pro/issues"
    },
    "homepage": "https://github.com/your-username/fleetiq-pro#readme"
  }